{
  "name": "beth-ts",
  "version": "1.0.0",
  "description": "A Better Ethereum library, written in TypeScript",
  "scripts": {
    "start": "node ./src/index.js",
    "lint": "tslint --project ./",
    "watch": "cross-env NODE_ENV=development webpack --watch --mode development --display-error-details --progress --color",
    "build:dev": "cross-env NODE_ENV=development webpack --mode development --display-error-details --progress --color",
    "build:prod": "cross-env NODE_ENV=production webpack --mode production --progress --color",
    "prepare": "# npm run lint && npm run build:dev"
  },
  "main": "./src/index.js",
  "dependencies": {
    "axios": "^0.18.0",
    "bignumber.js": "^7.2.1",
    "bn.js": "^4.11.8",
    "web3": "^1.0.0-beta.36"
  },
  "devDependencies": {
    "@babel/core": "7.1.0",
    "@types/chai": "4.1.5",
    "@types/chai-as-promised": "7.1.0",
    "@types/crypto-js": "3.1.43",
    "@types/mocha": "5.2.5",
    "@types/node": "10.10.3",
    "awesome-typescript-loader": "5.2.1",
    "babel-core": "6.26.3",
    "babel-loader": "8.0.2",
    "babel-polyfill": "6.26.0",
    "babel-preset-env": "1.7.0",
    "babel-preset-stage-3": "6.24.1",
    "cross-env": "5.2.0",
    "ts-loader": "5.1.1",
    "tslint": "5.11.0",
    "tslint-config-prettier": "1.15.0",
    "tslint-config-standard": "8.0.1",
    "tslint-eslint-rules": "5.4.0",
    "typedoc": "0.12.0",
    "typedoc-webpack-plugin": "1.1.4",
    "typescript": "3.0.3",
    "webpack": "4.19.1",
    "webpack-cli": "3.1.1",
    "webpack-node-externals": "1.7.2"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/republicprotocol/beth-ts.git"
  },
  "keywords": [
    "beth",
    "republic",
    "republicprotocol"
  ],
